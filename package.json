{
  "name": "chatbot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-staging": "ng build --configuration=staging",
    "watch": "ng build --watch --configuration development",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "ng lint",
    "format": "npx prettier . --write",
    "check-format": "npx prettier . --check",
    "build-prod-chatbot": "ng build",
    "build-staging-chatbot": "ng build --configuration=staging"
  },
  "lint-staged": {
    "*.{md,html,css,scss,js,jsx,mjs,cjs,ts,tsx,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,mjs,cjs,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "private": true,
  "dependencies": {
